datasource db {
  provider = "postgresql"
  url      = "postgresql://root:root@localhost:5432/search"
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-arm64-openssl-3.0.x"]
}

model SearchKeyword {
  id          String @id
  keyword     String @unique
  searchCount Int    @map("search_count")
  version     Int

  @@map("search_keyword")
}
